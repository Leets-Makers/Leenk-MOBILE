{
  "expo": {
    "name": "Leenk",
    "slug": "Leenk",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "@/assets/images/Logo_symbol.svg",
    "scheme": "leenk",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "@/assets/images/ic_splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#F7F7FA"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "@/assets/images/Logo_symbol.svg",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "@/assets/images/Logo_symbol.svg"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
